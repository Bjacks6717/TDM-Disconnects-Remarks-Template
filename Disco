function copyInternalExternal(inorex)
{
	c = document.getElementById("copyText");
	var sig = "";
	if(inorex == "external"){	sig = document.getElementById("signatureText").value; c.value += "\n\n\n";	}
	c.value += sig;
	c.select();
	document.execCommand('Copy');

}


</SCRIPT>
