<SCRIPT language="JavaScript" type="text/javascript">


var

myArray = new Array();
 
function g($elementToReturn) {
	return document.getElementById($elementToReturn);
}

function copy() {
	g('holdText').innerText = g('copyText').innerText;
	Copied = g('holdText').createTextRange();
	Copied.execCommand('Copy');
	g('msgSpace').innerHTML = "Copied to Clipboard";
	var t = setTimeout("g('msgSpace').innerHTML = \"\";", 3000);
}
function getCheckedValue(radioObj) {
	if(!radioObj)
		return "";
	var radioLength = radioObj.length;
	if(radioLength == undefined)
		if(radioObj.checked)
			return radioObj.value;
		else
			return "";
	for(var i = 0; i < radioLength; i++) {
		if(radioObj[i].checked) {
			return radioObj[i].value;
		}
	}
	
	return "";
}

function clicked($btn) {
	if ($btn == 'singletg') {
		var str = "***SEND TO STAGENB - include S7TR for SS7 in CORE." + 
			"\n***Project ID: " + g('projectid').value +
			"\n***This is a Green National Network Request." +
			"\n***Establish " + g('notg').value + " trunk group with " + g('ds0').value + " DS0 CHAN(s) from " + g('poi').value + 
			"\nto Lumen NGS " + g('sbc').value + " consuming the following LIMs Service ID(s):\n\n" + g('natds1').value +  
			"\n\nMessage Trunk with SS7.; " + 
			"\nOSP Name: " + g('osp').value + "; " + 
			"\nOSP Pt Code: " + g('osppc').value + "; " +
			"\nOSP Switch CLLI: " + g('ospswitch').value + "; " + 
			"\nOSP SS7 Hub Provider: " + g('ospss7').value + "; " + 
			"\nOSP NOC#: " + g('ospnoc').value + "; " + 
			"\n\n***Establish SS7 connectivity between " + g('sbc2').value + " to OSP Switch " + g('ospswitch').value + 
			" Pt Code " + g('osppc').value + ".***"; 
		g('copyText').value = str;
	}
	
    else {
		g('copyText').value = myArray[$btn];
    }
    copy();
}

</SCRIPT>

